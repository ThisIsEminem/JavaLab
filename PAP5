//WAP to create a vector using the numbers entered as command line agruements sort the vector and display it
import java.util.*;
				class Program5
				{
					public static void main(String args[])
					{
						Vector<Integer> v = new Vector<Integer>();
		
						int l=args.length,i,n;
						for(i=0;i<l;i++)
						{
							n=Integer.parseInt(args[i]);
							v.addElement(n);
						}
		
					System.out.println("Vector elements Before Sorting " +v.toString());
					Collections.sort(v);
					System.out.println("Vector elements After Sorting " +v.toString());
					}
				}
