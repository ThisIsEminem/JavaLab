//to find number of charcaters in each String
import java.util. * ;
import java.io.*;

class PBP6
{
	public static void main(String args[])
	{
		int i,j;
		Scanner sc = new Scanner(System. in );
		String arr[] = new String[10];
 		  
  		System.out.println("Enter 10 String");
		for (i = 0; i < 10; i++) 
		{
     			arr[i] = sc.next();
 		}
		System.out.println("String \t no of Characters");
       		for (i= 0; i< 10; i++) 
		{
			System.out.println(arr[i] + "\t" + arr[i].length());
		}
  	}
}
